<div class="cars">
  <div class="list">
    <h3>Listing Cars</h3>
    <%= link_to 'New car', new_car_path %>
    <table>
      <tr>
        <th>Title</th>
        <th>Type</th>
        <th>Color</th>
        <th>Image</th>
      </tr>
      <% @cars.each do |car| %>
        <tr>
          <td><%= car.title %></td>
          <td><%= car.type %></td>
          <td><%= car.color %></td>
          <td><%= image_tag car.image.url(:thumb), :class => "style_image" %></td>
          <td><%= link_to 'Show', car_path(car) %></td>
        </tr>
      <% end %>
    </table>
  </div>
 <%= render 'partials/links' %>
</div>
